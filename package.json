{
  "name": "sms-ts2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "cpFile": "cp -r ./build/public ./",
    "deploy": "npm run cpFile && npm run build && cd build && npm ci --production",
    "cp": "cp .env ./build",
    "test": "nodemon --ext 'ts' --exec 'clear && NODE_ENV=testing node -r @adonisjs/assembler/build/register japaFile.ts'",
    "test:all": "NODE_ENV=testing node -r @adonisjs/assembler/build/register japaFile.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.5",
    "@types/chai": "^4.2.21",
    "@types/lodash": "^4.14.172",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "adonis-preset-ts": "^2.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.1.1",
    "get-port": "^5.1.1",
    "japa": "^3.1.1",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.3.2",
    "sinon": "^11.1.2",
    "supertest": "^6.1.5",
    "typescript": "^4.2.4",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.7",
    "@adonisjs/core": "^5.1.11",
    "@adonisjs/lucid": "^15.0.3",
    "@adonisjs/repl": "^3.1.5",
    "@poppinss/utils": "^3.2.0",
    "axios": "^0.21.1",
    "camel-case": "^4.1.2",
    "del": "^6.0.0",
    "glob": "^7.1.7",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "pascal-case": "^3.1.2",
    "phc-argon2": "^1.1.2",
    "phc-bcrypt": "^1.0.7",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "zemen": "0.0.7",
    "zemen-qotari": "^1.0.4"
  }
}
